{
	"manifest_version": 2,
	"name": "HowFar",
	"version": "1.0",
	"description": "Enter an origin and destination address and get a map of directions using Google Maps",
	//"icons": ...
	"permissions": [
          "contextMenus"
        ],
	"browser_action": {
		"default_icon": "images/curved-road.jpg",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["backgroundpage.js", "jquery-3.0.0.min.js"]
	}
}